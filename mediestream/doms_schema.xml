        <!-- Boxes marking the article spatially -->
        <field name="alto_box" type="verbatim" stored="true" multiValued="true"/>

        <!-- Used for resource lookup -->
        <field name="thumbnailID"   type="keyword"  multiValued="false" stored="true" required="false"/>
        <!-- Resources that are no longer active (normally old presentation resources).
             Used to generate reports to CopyDan. -->
        <field name="inactiveResourceID"   type="keyword"  multiValued="true" stored="true" required="false"/>

