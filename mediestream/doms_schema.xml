        <field name="pwa" type="trie_float" multiValued="false"/>
        <field name="pwa_sort" type="verbatim" stored="true" multiValued="false"/>
        <!-- OCR Character Error Ratio -->
        <field name="cer" type="trie_float" multiValued="false"/>
