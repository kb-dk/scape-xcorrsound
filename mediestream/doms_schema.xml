        <field name="editionUUID"   type="keyword"  multiValued="false" stored="true" required="false"/>
        <!-- Authoritative ID for the page, taken unmodified from the DOMS record -->
        <field name="pageUUID"   type="keyword"  multiValued="false" stored="true" required="false"/>
        <field name="titleUUID"   type="keyword"  multiValued="false" stored="true" required="false"/>
        <field name="modsExist"   type="keyword"  multiValued="false" stored="true" required="false"/>

        <field name="familyId"   type="keyword"  multiValued="false" stored="true"/>
